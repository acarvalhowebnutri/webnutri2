<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"

    tools:context=".Nutricionista.PerfilNutricionistaFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">>

    <!-- <SearchView
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:id="@+id/search"/>
 -->


    <ImageView
        android:id="@+id/imageViewFotoNutricionista"
        android:layout_width="126dp"
        android:layout_height="80dp"
        android:layout_below="@+id/view2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:padding="0dip"
        android:src="@android:drawable/ic_menu_camera"
        tools:layout_conversion_absoluteHeight="80dp"
        tools:layout_conversion_absoluteWidth="126dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="27dp" />

    <TextView
        android:id="@+id/textViewExibeNOME"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewExibeCRN"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"
        android:layout_marginBottom="28dp"
        android:text="nome"
        android:textColor="#716f6f"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/textViewExibeCRN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewExibeEMAIL"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"
        android:layout_marginBottom="22dp"
        android:text="crn "
        android:textColor="#716f6f"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/textViewExibeEMAIL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonAlterar"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"
        android:layout_marginBottom="32dp"
        android:text="e-mail "
        android:textColor="#716f6f"
        android:textSize="22sp" />

    <Button
        android:id="@+id/buttonAlterar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="147dp"
        android:text="alterar"
        android:textColor="#499b3c" />


</RelativeLayout>